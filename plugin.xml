<?xml version='1.0' encoding='utf-8'?>
<plugin id="com-viatick-plugins-bms-sdk-public" version="0.1.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>BmsCordovaSdkPublic</name>
	<js-module name="BmsCordovaSdkPublic" src="www/BmsCordovaSdkPublic.js">
		<clobbers target="cordova.plugins.BmsCordovaSdkPublic" />
	</js-module>
	<platform name="ios">
		<config-file target="config.xml" parent="/*">
			<feature name="BmsCordovaSdkPublic">
				<param name="ios-package" value="BmsCordovaSdkPublic" />
			</feature>
		</config-file>
		<source-file src="src/ios/BmsCordovaSdkPublic.swift" />
		<source-file src="src/ios/SDK SRC/CONTROLLER/ViaApiCtrl.swift" target-dir="SDK SRC/CONTROLLER/" />
		<source-file src="src/ios/SDK SRC/CONTROLLER/ViaBackgroundTaskCenter.swift" target-dir="SDK SRC/CONTROLLER/" />
		<source-file src="src/ios/SDK SRC/CONTROLLER/ViaBmsCtrl.swift" target-dir="SDK SRC/CONTROLLER/" />
		<source-file src="src/ios/SDK SRC/CONTROLLER/ViaIBeaconCtrl.swift" target-dir="SDK SRC/CONTROLLER/" />
		<source-file src="src/ios/SDK SRC/CONTROLLER/ViaNotificationCenter.swift" target-dir="SDK SRC/CONTROLLER/" />
		<source-file src="src/ios/SDK SRC/VIEW/ViaMinisiteTableViewCell.swift" target-dir="SDK SRC/VIEW/" />
		<source-file src="src/ios/SDK SRC/VIEW/ViaMinisiteTableViewCell.xib" target-dir="SDK SRC/VIEW/" />
		<source-file src="src/ios/SDK SRC/VIEW/ViaMinisiteTableViewController.swift" target-dir="SDK SRC/VIEW/" />
		<source-file src="src/ios/SDK SRC/VIEW/ViaMinisiteTableViewController.xib" target-dir="SDK SRC/VIEW/" />
		<source-file src="src/ios/SDK SRC/VIEW/ViaMinisiteViewController.swift" target-dir="SDK SRC/VIEW/" />
		<source-file src="src/ios/SDK SRC/VIEW/ViaMinisiteViewController.xib" target-dir="SDK SRC/VIEW/" />
		<source-file src="src/ios/SDK SRC/MODEL/ViaBmsUtil.swift" target-dir="SDK SRC/MODEL/" />
		<source-file src="src/ios/SDK SRC/MODEL/ViaIBeacon.swift" target-dir="SDK SRC/MODEL/" />
		<source-file src="src/ios/SDK SRC/MODEL/ViaMinisite.swift" target-dir="SDK SRC/MODEL/" />
		<source-file src="src/ios/SDK SRC/RES/ViaIcons.xcassets" target-dir="SDK SRC/RES/" />
	</platform>
</plugin>
